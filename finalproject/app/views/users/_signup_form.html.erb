<p id="notice"><%= notice %></p>
<h1>Signup Page</h1>
<div>
  <%= form_for(@user) do |f| %>
    <%= f.label :Username %>
    <%= f.text_field :name %>
  
    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.label :password_confirmation %>
    <%= f.password_confirmation :password_confirmation %>

    <%= f.submit "Create Account" %>
<% end %>
</div>
