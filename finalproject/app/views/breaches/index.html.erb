<p id="notice"><%= notice %></p>
<strong><%= current_user.name %></strong>
<%= link_to 'log out', logout_path %> | 
<%= link_to 'edit account', edit_user_path(current_user) %>
<ul class="nav">
  <li><%= link_to 'Home', user_path(current_user) %></li>
</ul>

<h2>Listing Breaches</h2>

<table>
  <thead>
    <tr>
      <th>Active Breaches</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @breaches.each do |breach| %>
      <tr>
        <td><%= User.find(breach.target_id).name %></td>
        <td><%= link_to 'Show', breach %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Breach', new_breach_path %>
