<p id="notice"><%= notice %></p>

<h1>Listing Tag Objects</h1>

<table>
  <thead>
    <tr>
      <th>Str</th>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tag_objects.each do |tag_object| %>
      <tr>
        <td><%= tag_object.str %></td>
        <td><%= tag_object.image_id %></td>
        <td><%= link_to 'Show', tag_object %></td>
        <td><%= link_to 'Edit', edit_tag_object_path(tag_object) %></td>
        <td><%= link_to 'Destroy', tag_object, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tag object', new_tag_object_path %>
